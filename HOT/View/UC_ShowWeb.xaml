<UserControl x:Class="HOT.View.UC_ShowWeb"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HOT.View"
             xmlns:HOTControlLibrary="clr-namespace:HOTControlLibrary;assembly=HOTControlLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded">
    <Grid>
        <HOTControlLibrary:Loading Name="loading" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed" />
        <WebBrowser Name="Wb_showWeb" LoadCompleted="Wb_showWeb_LoadCompleted" />




    </Grid>
</UserControl>
